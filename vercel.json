{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "rewrites": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "destination": "/"
      }
    ]
  }
  